@model PTracking.ViewModel.MultipleData

<h2>Employee Data</h2>
<table>
    <thead>
        <tr>

            <th>Name</th>
            <th>Email</th>
            <!-- Add other employee properties as headers -->
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model.Employee)
        {
            <tr>

                <td>@employee.Name</td>
                <td>@employee.Email</td>
                <!-- Display other employee properties -->
            </tr>
        }
    </tbody>
</table>
